  "page": "\n<!DOCTYPE html>\n<html lang=\"en\">\n\n<head>\n    <meta charset=\"UTF-8\">\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Kids Fun</title>\n    <link rel=\"stylesheet\" href=\"https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css\">\n</head>\n\n<body>\n\n    <header class=\"container\">\n        <nav class=\"navbar navbar-expand-lg navbar-light bg-light\">\n            <a class=\"navbar-brand\" href=\"#\">Kids Fun</a>\n            <button class=\"navbar-toggler\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarSupportedContent\"\n                aria-controls=\"navbarSupportedContent\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\n                <span class=\"navbar-toggler-icon\"></span>\n            </button>\n\n            <div class=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\n                <ul class=\"navbar-nav ml-auto\">\n                    <li class=\"nav-item active\">\n                        <a class=\"nav-link\" href=\"#\">Home</a>\n                    </li>\n                    <li class=\"nav-item\">\n                        <a class=\"nav-link\" href=\"#\">About</a>\n                    </li>\n                    <li class=\"nav-item\">\n                        <a class=\"nav-link\" href=\"#\">Gallery</a>\n                    </li>\n                    <li class=\"nav-item\">\n                        <a class=\"nav-link\" href=\"#\">Contact</a>\n                    </li>\n                </ul>\n            </div>\n        </nav>\n    </header>\n\n    <div class=\"jumbotron\" style=\"background-color: lightblue;\">\n        <div class=\"container\">\n            <h1>Welcome to Kids Fun</h1>\n            <p>Let's have fun and learn together!</p>\n        </div>\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-md-4\">\n                <img src=\"https://source.unsplash.com/400x300/?kids\" class=\"img-fluid\" alt=\"Kids\">\n            </div>\n            <div class=\"col-md-4\">\n                <img src=\"https://source.unsplash.com/400x300/?playground\" class=\"img-fluid\" alt=\"Playground\">\n            </div>\n            <div class=\"col-md-4\">\n                <img src=\"https://source.unsplash.com/400x300/?toys\" class=\"img-fluid\" alt=\"Toys\">\n            </div>\n        </div>\n\n        <div class=\"text-center mt-4\">\n            <a href=\"#\" class=\"btn btn-primary\">Explore More</a>\n        </div>\n    </div>\n\n    <!-- Modified or New Request: Added Section with Form for Kids to Send Message -->\n\n    <section class=\"bg-light py-5\">\n        <div class=\"container\">\n            <h2 class=\"text-center mb-4\">Send Us a Message</h2>\n            <div class=\"row justify-content-center\">\n                <div class=\"col-md-8\">\n                    <form>\n                        <div class=\"form-group\">\n                            <label for=\"name\">Your Name</label>\n                            <input type=\"text\" class=\"form-control\" id=\"name\" placeholder=\"Enter your name\" required>\n                        </div>\n                        <div class=\"form-group\">\n                            <label for=\"email\">Your Email</label>\n                            <input type=\"email\" class=\"form-control\" id=\"email\" placeholder=\"Enter your email\" required>\n                        </div>\n                        <div class=\"form-group\">\n                            <label for=\"message\">Your Message</label>\n                            <textarea class=\"form-control\" id=\"message\" rows=\"3\" placeholder=\"Enter your message\"\n                                required></textarea>\n                        </div>\n                        <button type=\"submit\" class=\"btn btn-primary\">Send Message</button>\n                    </form>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <footer class=\"footer mt-4 py-3 text-center bg-dark text-white\">\n        Designed by Automated AI Robo B & D\n    </footer>\n\n    <script src=\"https://code.jquery.com/jquery-3.5.1.slim.min.js\"></script>\n    <script src=\"https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.4/dist/umd/popper.min.js\"></script>\n    <script src=\"https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js\"></script>\n</body>\n\n</html>"
